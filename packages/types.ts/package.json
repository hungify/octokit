{
  "name": "octokit-types",
  "module": "scripts/build.mjs",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "update-endpoints": "npm-run-all update-endpoints:*",
    "update-endpoints:fetch-json": "VERSION=8.0.0 node scripts/update-endpoints/fetch-json.mjs",
    "update-endpoints:typescript": "VERSION=8.0.0 bun run scripts/update-endpoints/typescript.mjs",
    "build:types": "bun run scripts/build.mjs && tsc -p tsconfig.json"
  },
  "dependencies": {
    "github-openapi-graphql-query": "^4.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "handlebars": "^4.7.8",
    "json-schema-to-typescript": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sort-keys": "^5.0.0",
    "#openapi-types.ts": "link:openapi-types.ts"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
